#!/bin/sh
# Copyright (C) 1989-2022 PC2 Development Team: John Clevenger, Douglas Lane, Samir Ashoo, and Troy Boudreau.
#
# Purpose: start the Testing Tool UI
# Author : pc2@ecs.csus.edu
#

. "$(dirname "$0")/pc2env"

# MacOS or not
if [ -f /mach_kernel ]; then
  # set our dock name (otherwise defaults to Starter)
  PC2XOPS="-Xdock:name=$(basename "$0")"
else
  PC2XOPS=""
fi

java -Djdk.crypto.KeyAgreement.legacyKDF=true $PC2XOPS -Xms64M -Xmx768M -cp "$pc2_classpath" edu.csus.ecs.pc2.Starter --ui edu.csus.ecs.pc2.ui.admin.TestingToolView "$@"

# eof pc2rui $Id$
